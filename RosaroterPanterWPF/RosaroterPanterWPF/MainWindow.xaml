<Window x:Class="RosaroterPanterWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RosaroterPanterWPF"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:RosaroterPanterWPF"
        Title="Project" Height="720" Width="1280">
	<Window.DataContext>
		<vm:MainViewModel x:Name="model" />
	</Window.DataContext>
	<Grid Opacity="0.5">
		<Label Content="Test text" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Center" Width="497" HorizontalContentAlignment="Center"/>
		<Button x:Name="HomeButton" HorizontalAlignment="Left" VerticalAlignment="Top" Width="40" Margin="10,10,0,0" RenderTransformOrigin="-0.082,0.52" Height="40" BorderBrush="{x:Null}">
			<Button.Background>
				<ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/home.png"/>
			</Button.Background>
		</Button>

	</Grid>
</Window>
