<Window x:Class="RosaroterPanterWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RosaroterPanterWPF"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:RosaroterPanterWPF"
        Title="{Binding Title}" Height="720" Width="1280">
	<Window.DataContext>
		<vm:MainViewModel x:Name="model" />
	</Window.DataContext>
	<Grid Grid.Column="3" Grid.Row="3">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Content="Button " Grid.Column="0" Grid.Row="0" IsEnabled="True" Command="{Binding StartTimerCommand}" />
        <Label Content="{Binding TimerSeconds}" Grid.Row="1" Grid.Column="0" />

    </Grid>
</Window>
